{
  "CreateMap": {
    "prefix": "esri-create-map",
    "body": [
      "import Map = require(\"esri/Map\");",
      "import MapView = require(\"esri/views/MapView\");",
      "const map = new Map({",
      "\tbasemap: \"${1:streets}\"",
      "});",
      "const view = new MapView({",
      "\tmap,",
      "\tcontainer:\"viewDiv\",",
      "\tcenter:[${2: -118.244,34.052}],",
      "\tzoom:${3:12}",
      "});"
    ],
    "description": "Create Map TypeScript"
  },
  "CreateScene": {
    "prefix": "esri-create-scene",
